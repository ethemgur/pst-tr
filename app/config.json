    "projectId": "pstherapy-2",
    "allowEmailLogin": true,
    "allowEmailRegistration": true
  },
  "loginRequiredForAllPages": false,
  "appStoreId": "com.aea.pstherapy",
  "playStoreId": "com.aea.pstherapy",
  "useCordovaPlugins": [],
  "cordovaPreferences": {
    "DisallowOverscroll": true,
    "Orientation": "portrait"
  },
  "resetLocalStorageOnVersionChange": false,
  "preloadImages": true,
  "buildSourcemaps": false,
  "editorConfig": {
    "root": "true",
    "[*]": null,
    "indent_style": "space",
    "indent_size": "2",
    "charset": "utf-8",
    "trim_trailing_whitespace": "true",
    "insert_final_newline": "true",
    "end_of_line": "lf",
    "max_line_length": "null"
  },
  "eslint": {
    "extends": "airbnb",
    "rules": {
      "max-len": 0,
      "no-unused-vars": 0,
      "no-plusplus": 0,
      "no-param-reassign": 0,
      "prefer-destructuring": 0,
      "guard-for-in": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-loop-func": 0,
      "no-restricted-syntax": 0,
      "no-prototype-builtins": 0,
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "fixCodeOnTest": true,
  "fixCodeOnBuild": true,
  "devServerPort": "8080",
  "gitignore": {},
  "npmignore": {},
  "debug": false
}
